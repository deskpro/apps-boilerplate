{
  "name": "@deskpro/apps-boilerplate",
  "version": "0.2.3",
  "description": "My first app",
  "main": "lib/main/javascript/index.js",
  "scripts": {
    "dev": "dpat server .",
    "package": "dpat clean . && dpat compile . && dpat bundle-installer . && dpat verify ./dist && dpat package .",
    "test": "dpat test .",
    "symlink-webpack-binaries": "cd ./node_modules/@deskpro/apps-dpat/node_modules/.bin && rm -rf webpack && rm -rf webpack-dev-server &&  ln -sf ../webpack-dev-server/bin/webpack-dev-server.js webpack-dev-server && ln -sf ../webpack/bin/webpack.js webpack",
    "postinstall": "npm run symlink-webpack-binaries"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deskpro/apps-boilerplate.git"
  },
  "keywords": [
    "deskpro",
    "apps boilerplate"
  ],
  "author": {
    "name": "Deskpro Apps",
    "email": "apps@deskpro.com",
    "url": "https://www.deskpro.com"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/deskpro/apps-boilerplate/issues"
  },
  "homepage": "https://github.com/deskpro/apps-boilerplate#readme",
  "deskpro": {
    "version": "2.3.0",
    "title": "My first app",
    "isSingle": true,
    "scope": "agent",
    "targets": [
      {
        "target": "ticket-sidebar",
        "url": "html/index.html"
      }
    ],
    "storage": [],
    "settings": [],
    "customFields": [],
    "deskproApiTags": [],
    "externalApis": []
  },
  "devDependencies": {
    "@deskpro/apps-sdk-react": "^0.2.12",
    "@deskpro/apps-sdk-core": "^1.0.0-beta.28",
    "@deskpro/react-components": "^1.3.18",
    "@deskpro/redux-components": "^1.0.3",
    "@deskpro/apps-dpat": "^0.10.0",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1"
  }
}
